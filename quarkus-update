#!/bin/bash

wget https://github.com/adoptium/temurin17-binaries/releases/latest/download/OpenJDK17U-jdk_x64_linux_hotspot_17.0.7_7.tar.gz
tar xzf OpenJDK17U-jdk_x64_linux_hotspot_17.0.7_7.tar.gz
mv jdk-17.0.7+7 ~/temurin-17
export JAVA_HOME=~/temurin-17
export PATH=$JAVA_HOME/bin:$PATH
source ~/.bashrc

java -version

./mvnw quarkus:update -Dstream=3.3